<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools">

    <com.nyotek.dot.admin.widgets.NSConstraintLayout
        android:id="@+id/cl_dispatch_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_8sdp"
        android:layout_marginTop="@dimen/_8sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <!-- Top Detail -->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_top_detail"
            android:layout_width="0dp"
            android:layout_height="@dimen/_18sdp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <!-- View Line Decoration -->
            <com.nyotek.dot.admin.widgets.NSViewLine
                android:id="@+id/view_line_decoration"
                android:layout_width="@dimen/_1sdp"
                android:layout_height="@dimen/_5sdp"
                android:background="@color/line_divider"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <!-- ImageView for Hubz Icon -->
            <ImageView
                android:id="@+id/iv_hubz_icon"
                android:layout_width="@dimen/_9sdp"
                android:layout_height="@dimen/_9sdp"
                android:layout_marginStart="@dimen/_5sdp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <!-- TextView for Dispatch View Title -->
            <com.nyotek.dot.admin.widgets.NSPrimaryTextView
                android:id="@+id/dispatch_view_title"
                style="@style/LayoutWidthHeightZeroWrap"
                android:layout_marginStart="@dimen/_3sdp"
                android:gravity="start"
                android:textAlignment="viewStart"
                android:textSize="@dimen/_5sdp"
                app:layout_constraintBottom_toBottomOf="@id/iv_hubz_icon"
                app:layout_constraintEnd_toStartOf="@id/tv_order_places"
                app:layout_constraintStart_toEndOf="@id/iv_hubz_icon"
                app:layout_constraintTop_toTopOf="@id/iv_hubz_icon" />

            <!-- TextView for Order Places -->
            <com.nyotek.dot.admin.widgets.NSWhiteTextView
                android:id="@+id/tv_order_places"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_4sdp"
                android:paddingHorizontal="@dimen/_4sdp"
                android:paddingVertical="@dimen/_1sdp"
                android:textSize="@dimen/_4sdp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/iv_hubz_icon" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <!-- View Line Top Assigned -->
        <View
            android:id="@+id/view_line_top_assigned"
            android:layout_width="match_parent"
            android:layout_height="0.6dp"
            android:background="@color/line_divider"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cl_top_detail" />

        <!-- ConstraintLayout for Driver Item -->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_driver_item"
            android:layout_width="0dp"
            android:layout_height="@dimen/_20sdp"
            android:layout_marginHorizontal="@dimen/_4sdp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view_line_top_assigned">

            <!-- ImageView for Driver Image -->
            <com.nyotek.dot.admin.widgets.ImageIconView
                android:id="@+id/iv_driver_img"
                android:layout_width="@dimen/_10sdp"
                android:layout_height="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_1sdp"
                android:padding="@dimen/_2sdp"
                android:src="@drawable/ic_dispatch_user"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <!-- TextView for Driver Title -->
            <com.nyotek.dot.admin.widgets.NSPrimaryTextView
                android:id="@+id/tv_driver_title"
                style="@style/LayoutWidthHeightZeroWrap"
                android:layout_marginStart="@dimen/_4sdp"
                android:paddingStart="@dimen/_4sdp"
                android:paddingEnd="@dimen/_4sdp"
                android:textSize="@dimen/_5sdp"
                app:layout_constraintBottom_toTopOf="@id/tv_description"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/iv_driver_img"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed" />

            <!-- TextView for Description -->
            <com.nyotek.dot.admin.widgets.NSPrimaryLightTextView
                android:id="@+id/tv_description"
                style="@style/LayoutWidthHeightZeroWrap"
                android:layout_marginStart="@dimen/_4sdp"
                android:layout_marginTop="@dimen/_2sdp"
                android:paddingStart="@dimen/_4sdp"
                android:paddingEnd="@dimen/_5sdp"
                android:textSize="@dimen/_4ssp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/iv_driver_img"
                app:layout_constraintTop_toBottomOf="@id/tv_driver_title" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <!-- View Line Second Assigned -->
        <View
            android:id="@+id/view_line_second_assigned"
            android:layout_width="match_parent"
            android:layout_height="0.6dp"
            android:background="@color/line_divider"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cl_driver_item" />

        <!-- ConstraintLayout for Model Item -->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_model_item"
            android:layout_width="0dp"
            android:layout_height="@dimen/_20sdp"
            android:layout_marginHorizontal="@dimen/_4sdp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view_line_second_assigned">

            <!-- ImageView for Model Image -->
            <com.nyotek.dot.admin.widgets.ImageIconView
                android:id="@+id/iv_model_img"
                android:layout_width="@dimen/_10sdp"
                android:layout_height="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_1sdp"
                android:padding="@dimen/_2sdp"
                android:src="@drawable/ic_driver_handle"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <!-- TextView for Model Title -->
            <com.nyotek.dot.admin.widgets.NSPrimaryTextView
                android:id="@+id/tv_model_title"
                style="@style/LayoutWidthHeightZeroWrap"
                android:layout_marginStart="@dimen/_4sdp"
                android:paddingStart="@dimen/_4sdp"
                android:paddingEnd="@dimen/_4sdp"
                android:textSize="@dimen/_5sdp"
                android:textAlignment="viewStart"
                app:layout_constraintBottom_toTopOf="@id/tv_model_description"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/iv_model_img"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed" />

            <!-- TextView for Model Description -->
            <com.nyotek.dot.admin.widgets.NSPrimaryLightTextView
                android:id="@+id/tv_model_description"
                style="@style/LayoutWidthHeightZeroWrap"
                android:layout_marginTop="@dimen/_2sdp"
                android:paddingStart="@dimen/_4sdp"
                android:paddingEnd="@dimen/_5sdp"
                android:textSize="@dimen/_4ssp"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/iv_model_img"
                app:layout_constraintTop_toBottomOf="@id/tv_model_title" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <!-- View Line Model Assigned -->
        <View
            android:id="@+id/view_line_model_assigned"
            android:layout_width="match_parent"
            android:layout_height="0.6dp"
            android:background="@color/line_divider"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cl_model_item" />

        <!-- ConstraintLayout for Dispatch Detail View -->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_dispatch_detail_view"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_55sdp"
            android:layout_marginHorizontal="@dimen/_5sdp"
            android:layout_marginTop="@dimen/_4sdp"
            android:layout_marginBottom="@dimen/_4sdp"
            android:paddingVertical="@dimen/_2sdp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/view_line_model_assigned">

            <!-- ImageView for Product Icon Round -->
            <ImageView
                android:id="@+id/iv_product_icon_round"
                android:layout_width="@dimen/_9sdp"
                android:layout_height="@dimen/_9sdp"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_2sdp"
                android:src="@drawable/ic_place_holder_product"
                android:visibility="gone"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.nyotek.dot.admin.widgets.NSBlackTextView
                android:id="@+id/tv_dispatch_title"
                style="@style/LayoutWidthHeightWrap"
                android:layout_marginStart="@dimen/_4sdp"
                android:layout_marginTop="@dimen/_3sdp"
                android:fontFamily="@font/archivo_medium"
                android:gravity="start"
                android:textAlignment="viewStart"
                android:textSize="@dimen/_4sdp"
                tools:text="Order Id:"
                app:layout_constraintStart_toEndOf="@id/iv_product_icon_round"
                app:layout_constraintTop_toTopOf="parent" />

            <!-- TextView for Order ID -->
            <com.nyotek.dot.admin.widgets.NSPrimaryTextView
                android:id="@+id/tv_dispatch_id"
                style="@style/LayoutWidthHeightZeroWrap"
                android:fontFamily="@font/archivo_semi_bold"
                android:gravity="start"
                android:textAlignment="viewStart"
                android:textSize="@dimen/_5sdp"
                tools:text="#12233434"
                android:layout_marginTop="@dimen/_2sdp"
                app:layout_constraintEnd_toStartOf="@id/view_line_horizontal_divider"
                app:layout_constraintStart_toStartOf="@id/tv_dispatch_title"
                app:layout_constraintTop_toBottomOf="@id/tv_dispatch_title" />

            <!-- TextView for Order Title -->
            <com.nyotek.dot.admin.widgets.NSBlackTextView
                android:id="@+id/tv_order_title"
                style="@style/LayoutWidthHeightWrap"
                android:layout_marginStart="@dimen/_4sdp"
                android:layout_marginTop="@dimen/_3sdp"
                android:fontFamily="@font/archivo_medium"
                android:gravity="start"
                android:textAlignment="viewStart"
                android:textSize="@dimen/_4sdp"
                tools:text="Order Id:"
                app:layout_constraintStart_toEndOf="@id/view_line_horizontal_divider"
                app:layout_constraintTop_toTopOf="parent" />

            <!-- TextView for Order ID -->
            <com.nyotek.dot.admin.widgets.NSPrimaryTextView
                android:id="@+id/tv_order_id"
                style="@style/LayoutWidthHeightZeroWrap"
                android:fontFamily="@font/archivo_semi_bold"
                android:gravity="start"
                android:textAlignment="viewStart"
                android:textSize="@dimen/_5sdp"
                tools:text="#12233434"
                android:layout_marginTop="@dimen/_2sdp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/tv_order_title"
                app:layout_constraintTop_toBottomOf="@id/tv_order_title" />

            <View
                android:id="@+id/view_line_horizontal_divider"
                android:layout_width="0.6dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toTopOf="@id/view_line_divider"/>

            <!-- View Line Divider -->
            <View
                android:id="@+id/view_line_divider"
                android:layout_width="match_parent"
                android:layout_height="0.6dp"
                android:layout_marginTop="@dimen/_4sdp"
                app:layout_constraintTop_toBottomOf="@id/tv_order_id" />

            <!-- ImageView for Location Start -->
            <ImageView
                android:id="@+id/iv_location_start"
                android:layout_width="@dimen/_7sdp"
                android:layout_height="@dimen/_7sdp"
                android:layout_marginStart="@dimen/_5sdp"
                android:src="@drawable/ic_location"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/tv_starting_location" />

            <!-- View Line Location Start -->
            <View
                android:id="@+id/view_line_location_start"
                android:layout_width="0.6dp"
                android:layout_height="0dp"
                android:layout_marginVertical="@dimen/_1sdp"
                android:layout_marginStart="@dimen/_3sdp"
                android:background="@color/line_divider"
                app:layout_constraintBottom_toBottomOf="@id/iv_location_start"
                app:layout_constraintStart_toEndOf="@id/iv_location_start"
                app:layout_constraintTop_toTopOf="@id/iv_location_start" />

            <!-- TextView for Starting Location -->
            <com.nyotek.dot.admin.widgets.NSPrimaryTextView
                android:id="@+id/tv_starting_location"
                style="@style/LayoutWidthHeightZeroWrap"
                android:layout_marginStart="@dimen/_3sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginEnd="@dimen/_4sdp"
                android:fontFamily="@font/archivo_medium"
                android:gravity="start"
                android:includeFontPadding="false"
                android:lineSpacingExtra="@dimen/_1sdp"
                android:lines="2"
                android:paddingTop="@dimen/_2sdp"
                android:paddingBottom="0dp"
                android:textAlignment="viewStart"
                android:textSize="@dimen/_4sdp"
                app:layout_constraintBottom_toTopOf="@id/tv_ending_location"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/view_line_location_start"
                app:layout_constraintTop_toBottomOf="@+id/view_line_divider" />

            <!-- ImageView for Location End -->
            <ImageView
                android:id="@+id/iv_location_end"
                android:layout_width="@dimen/_7sdp"
                android:layout_height="@dimen/_7sdp"
                android:layout_marginStart="@dimen/_5sdp"
                android:src="@drawable/ic_location_end"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/tv_ending_location" />

            <!-- View Line Location End -->
            <View
                android:id="@+id/view_line_location_end"
                android:layout_width="0.6dp"
                android:layout_height="0dp"
                android:layout_marginVertical="@dimen/_1sdp"
                android:layout_marginStart="@dimen/_3sdp"
                android:background="@color/line_divider"
                app:layout_constraintBottom_toBottomOf="@id/iv_location_end"
                app:layout_constraintStart_toEndOf="@id/iv_location_end"
                app:layout_constraintTop_toTopOf="@id/iv_location_end" />

            <!-- TextView for Ending Location -->
            <com.nyotek.dot.admin.widgets.NSPrimaryTextView
                android:id="@+id/tv_ending_location"
                style="@style/LayoutWidthHeightZeroWrap"
                android:layout_marginStart="@dimen/_3sdp"
                android:layout_marginTop="@dimen/_3sdp"
                android:layout_marginEnd="@dimen/_4sdp"
                android:fontFamily="@font/archivo_medium"
                android:gravity="start"
                android:includeFontPadding="false"
                android:lineSpacingExtra="@dimen/_1sdp"
                android:lines="2"
                android:paddingTop="@dimen/_1sdp"
                android:paddingBottom="0dp"
                android:textAlignment="viewStart"
                android:textSize="@dimen/_4sdp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/view_line_location_end"
                app:layout_constraintTop_toBottomOf="@+id/tv_starting_location" />

            <!-- ImageView for Line Starting Ending -->
            <com.nyotek.dot.admin.widgets.ImageIconView
                android:id="@+id/view_line_starting_ending"
                android:layout_width="@dimen/_2sdp"
                android:layout_height="0dp"
                android:layout_marginVertical="@dimen/_2sdp"
                android:src="@drawable/ic_dash_line"
                app:layout_constraintBottom_toTopOf="@id/iv_location_end"
                app:layout_constraintEnd_toEndOf="@id/iv_location_start"
                app:layout_constraintStart_toStartOf="@id/iv_location_start"
                app:layout_constraintTop_toBottomOf="@id/iv_location_start" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </com.nyotek.dot.admin.widgets.NSConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>